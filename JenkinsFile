pipeline {
    agent any

    
    stages {
        stage('Compile Stage') {
            steps {
                bat "mvn clean compile"
            }
        }
        stage('Starting Runtime') {
            steps {
                bat "mule"
            }
        }                   	
        stage('test Stage') {
            steps {
                bat "mvn test -DmuleDeploy"
            }
        }        
    }
}
